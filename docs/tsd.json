{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["src/motor"],
        "exclude": ["src/motor/wasm"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "template": "./node_modules/tsd-jsdoc/dist"
    }
}