{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["src/motor"],
        "exclude": ["src/motor/wasm"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "tutorials": "tutorial"
    },
    "templates": {
        "logo": {
            "url": "./logo.png",
            "width": "156px",
            "height": "36px"
        },
        "name": "MotorJS",
        "footerText": " ",
        "tabNames": {
            "api": "API",
            "tutorials": "教程"
        },
        "default": {
            "staticFiles": {
                "include": ["docConfig/"]
            },
            "outputSourceFiles": false
        },
        "css": [
            "custom.css"
        ]
    }
}